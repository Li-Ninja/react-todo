{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "react/jsx-props-no-spreading": "off",
    "import/prefer-default-export": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}